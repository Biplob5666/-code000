{
  "name": "kali-like-codespace",
  "build": { "dockerfile": "Dockerfile", "context": ".." },
  "postCreateCommand": "/workspaces/${localWorkspaceFolderBasename}/.devcontainer/post-create.sh",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers",
        "ms-python.python",
        "eamodio.gitlens"
      ]
    }
  },
  "remoteUser": "vscode"
}
